{
  "name": "newquillboard",
  "private": true,
  "version": "1.0.0",
  "description": "Full-stack QuillBoard project (NestJS backend + Next.js frontend)",
  "scripts": {
    "start:backend": "cd backend && npx nest start --watch",
    "start:frontend": "cd frontend && npx next dev",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build:backend": "cd backend && npx nest build",
    "build:frontend": "cd frontend && npx next build",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down"
  },
  "main": "index.js",
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-socket.io": "^7.6.18",
    "@nestjs/websockets": "^7.6.18",
    "@prisma/client": "^5.14.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.19.2",
    "next": "^16.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.23",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/socket.io": "^3.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.0",
    "postcss": "^8.5.6",
    "prisma": "^5.14.0",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3"
  },
